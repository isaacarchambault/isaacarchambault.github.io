@charset "UTF-8";

/* images */

.img {
	width: 100%;
	height: auto;
	-webkit-box-shadow: 10px 10px 10px  rgba(0, 0, 0, 0.3);
  	-moz-box-shadow: 10px 10px 10px rgba(0, 0, 0, 0.3);
	box-shadow:  10px 10px 10px rgba(0, 0, 0, 0.3);
	opacity: 1.0;
}
@media only screen and (max-width: 1000px) {
  .img {
    -webkit-box-shadow: 10px 10px 10px  rgba(0, 0, 0, 0.0);
  	-moz-box-shadow:    10px 10px 10px rgba(0, 0, 0, 0.0);
	box-shadow:  10px 10px 10px rgba(0, 0, 0, 0.0);
  }
}
.imgalt {
	width: 100%;
	height: auto;
	-webkit-filter: drop-shadow(10px 10px 10px  rgba(0, 0, 0, 0.3));
	filter: drop-shadow(10px 10px 10px rgba(0, 0, 0, 0.3));
	opacity: 1.0;
}
@media only screen and (max-width: 1000px) {
  .imgalt {
   -webkit-filter: drop-shadow(10px 10px 10px  rgba(0, 0, 0, 0.0));
	filter: drop-shadow(10px 10px 10px rgba(0, 0, 0, 0.0));
  }
}
.imgaltalt {
	width: 100%;
	height: auto;
	opacity: 1.0;
}
.self {
	width: 50%;
	height: auto;
	margin-left: auto;
	margin-right: auto;
	display: block;
	position: relative;
	opacity: 1.0;
}
#logolarge {
	margin-left: auto;
	margin-right: auto;
	margin-top: 20px;
	left: 0;
	right: 0;
	dosplay: block;
	position: absolute;
	width: 350px;
	height: auto;
	-webkit-transition: 1s ease-in-out;
	transition: 1s ease-in-out;
	animation: 1s ease-out 0s 1 slideUp;
}
@media only screen and (max-width: 2000px) {
  #logolarge {
    width: 350px;
	margin-top: 20px;
  }
}
@media only screen and (max-width: 1500px) {
  #logolarge {
    width: 250px;
	margin-top: 10px;
  }
}
@media only screen and (max-width: 1000px) {
  #logolarge {
    width: 200px;
  }
}
#wallpaper {
	width: 100%;
	height: auto;
	margin-left: auto;
	margin-right: auto;
	display: block;
	position: fixed;
	z-index: -100;
}
@media only screen and (max-width: 1000px) {
  #wallpaper {
	z-index: 30;
	position: absolute;
	min-width: 800px;
  }
}
