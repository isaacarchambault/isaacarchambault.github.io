@charset "UTF-8";

/* Design Styles */

body {
	width: 100%;
	height: auto;
	margin-left: auto;
	margin-right: auto;
	margin-top: 0px;
	margin-bottom: 0px;
	background-color: white;
	overflow-x: hidden;
}
@media only screen and (max-width: 800px) {
  body {
    min-width: 800px;
  }
}
.divboxSpacer {
	margin-top: 20px;
	margin-bottom: 20px;
}.divboxSpacer2 {
	margin-top: 100px;
	margin-bottom: 20px;
}


/*Design ID's */

#divbox1 {
	margin-right: auto;
	margin-left: auto;
	top: 1000px;
	margin-bottom: 0px;
	padding-bottom: auto;
	display: block;
	position: relative;
	width: 100%;
	height: auto;
	z-index: 10;
	background-color: white;
	overflow-x: hidden;
	-webkit-transition: 0.7s ease-in-out;
	transition: 0.7s ease-in-out;
}
@media only screen and (max-width: 2000px) {
  #divbox1 {
    top: 950px;
  }
}
@media only screen and (max-width: 1600px) {
  #divbox1 {
    top: 750px;
  }
}
@media only screen and (max-width: 1300px) {
  #divbox1 {
    top: 600px;
  }
}
@media only screen and (max-width: 1050px) {
  #divbox1 {
	-webkit-transition: 0.0s ease-in-out;
	transition: 0.0s ease-in-out;  
  }
}
@media only screen and (max-width: 1000px) {
  #divbox1 {
    top: 75px;
	position: relative;
	-webkit-transition: 0.0s ease-in-out;
	transition: 0.0s ease-in-out;  
  }
}
