@charset "UTF-8";
/* header */
#header {
	width: 100%;
	height: 100px;
	background-color: white;
	display: block;
	z-index: 100;
	position: fixed;
	border-bottom: 10px #3e6897;
	border-bottom-style: solid;
	-webkit-box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.5);
	-moz-box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.5);
	box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.5);
	-webkit-transition: 0.5s ease-in-out;
	transition: 0.5s ease-in-out;
}
@media only screen and (max-width: 1000px) {
  #header {
    height: 350px;
	position: relative;
  }
}
#headerbox {
	margin-left: auto;
	margin-right: auto;
	display: block;
	left: 0;
	right: 0;
	top: 100px;
	position: fixed;
	background-color: transparent;
}
@media only screen and (max-width: 1000px) {
  #headerbox {
    position: absolute;
	z-index: 100;
	min-width: 800px;
  }
}
#headFix {
	position: fixed;
	right: 0;
	left: 0;
}
@media only screen and (max-width: 1000px) {
  #headFix {
	z-index: 30;
	position: absolute;
  }
}
.headerListitems {
	margin-top: 40px;
	margin-right: 50px;
	float: right;
	-webkit-transition: 0.1s ease-in;
	transition: 0.1s ease-in;
}
.headerListitemssmall {
	z-index: -1;
	margin-left: auto;
	margin-right: 30px;
	left: 0;
	right: 0;
	text-align: center;
	position: absolute;
	margin-top: 40px;
	-webkit-transition: 0.1s ease-in;
	transition: 0.1s ease-in;
}
@media only screen and (min-width: 1000px) {
.headerListitemssmall {
    display: none;
  }
}
.headerStaticText {
	margin-top: 40px;
	margin-right: 50px;
	float: left;
	-webkit-transition: 0.1s ease-in;
	transition: 0.1s ease-in;
}
@keyframes slideRight {
  0% {
    transform: translateX(-100%);
	opacity: 0.0;
  }
  100% {
    transform: translateX(0);
	opacity: 1.0;
  }
}
.smallLogo {
	margin-left: 50px;
	margin-top: 10px;
	width: 80px;
	height: auto;
	float: left;
	-webkit-transition: 0.1s ease-in-out;
	transition: 0.1s ease-in-out;
	animation: 1s ease-out 0s 1 slideRight;
}
@media only screen and (max-width: 1000px) {
 .smallLogo {
    opacity: 0.0;
	z-index: -1;
	animation: none;
  }
}
#smallmenuicon {
	margin-left: auto;
	margin-right: auto;
	left: 0;
	right: 0;
	margin-top: 120px;
	width: 120px;
	height: auto;
	position: absolute;
	opacity: 0.0;
	z-index: -100;
	-webkit-transition: 0.4s ease-in-out;
	transition: 0.4s ease-in-out;
}
@media only screen and (max-width: 1000px) {
 #smallmenuicon {
    opacity: 1.0;
	z-index: 100;
  }
}
@media only screen and (max-width: 1000px) {
 #smallmenuicon {
    cursor: pointer;
  }
}
#statictext {
	display: block;
	opacity: 1.0;
}
@media only screen and (max-width: 1000px) {
#statictext {
    display: none;
  }
}
#menuitems {
	display: block;
	opacity: 1.0;
}
@media only screen and (max-width: 1000px) {
#menuitems {
    display: none;
  }
}
#menuclicker {
	display: block;
	z-index: 200;
}
#smallmenucontroller {
	display: none;
}
#smallmenuiconcontroller {
	display: none;
}
@media only screen and (max-width: 1000px) {
#smallmenuiconcontroller {
    display: block;
  }
}
